mixin navItem(icon)
	li
		a
			i.fa.fa-lg(class=icon)

nav.navbar.navbar-default(role='navigation')
	div.container-fluid
		div.navbar-header
			button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
				span.sr-only Toggle navigation
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href='#')
				i.fa.fa-slideshare.fa-lg
		div.collapse.navbar-collapse#bs-example-navbar-collapse-1
			ul.nav.navbar-nav.navbar-left
				+navItem('fa-android')
				+navItem('fa-apple')
				+navItem('fa-github')
				+navItem('fa-google')
				+navItem('fa-trello')
				+navItem('fa-stack-overflow')
				+navItem('fa-jsfiddle')
				+navItem('fa-youtube')
				+navItem('fa-html5')
				+navItem('fa-css3')
			ul.nav.navbar-nav.navbar-right
				+navItem('fa-facebook')
				+navItem('fa-twitter')
				+navItem('fa-reddit')
				+navItem('fa-linkedin')
				+navItem('fa-instagram')
				+navItem('fa-tumbler')
				